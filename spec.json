{
  "nio/BACNetClientBlock": {
    "version": "0.1.0",
    "description": "Blocks to read from BACNet devices on an IP network",
    "categories": [],
    "properties": {
      "address": {
        "title": "IP Address",
        "type": "StringType",
        "description": "IPv4 address of BACnet device",
        "default": "127.0.0.1"
      },
      "array_index": {
        "title": "Data index",
        "type": "IntType",
        "description": "Index of data",
        "default": null
      },
      "instance": {
        "title": "Object Instance Number",
        "type": "IntType",
        "description": "BACNet object instance number",
        "default": null
      },
      "max_apdu_length": {
        "title": "Max APDU Length",
        "type": "IntType",
        "description": "Max APDU length in bytes",
        "default": 1024
      },
      "property": {
        "title": "Name of Property",
        "type": "StringType",
        "description": "Name of property to be read",
